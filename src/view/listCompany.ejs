<div class="container mt-5">
    <div class="row">
        <div class="col-md-12">
            <h2 class="text-center">Company List</h2>
            <a href="/addcompany" class="btn btn-primary">Add company</a>
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Company Name</th>
                        <th>Industry</th>
                    </tr>
                </thead>
                <tbody>
                    <% companies.forEach(company=> { %>
                        <tr>
                            <td>
                                <%= company.name %>
                            </td>
                            <td>
                                <%= company.industry %>
                            </td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
        </div>
    </div>
</div>